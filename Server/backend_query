
-- for user TABLE

CREATE TABLE `cricket_app`.`users` (`id` INT NOT NULL AUTO_INCREMENT , `name` VARCHAR(100) NULL DEFAULT NULL , `gender` VARCHAR(20) NULL DEFAULT NULL , `email` VARCHAR(255) NULL DEFAULT NULL , `number` VARCHAR(20) NULL DEFAULT NULL , `dob` VARCHAR(20) NULL DEFAULT NULL , `address` TEXT NOT NULL , `city` VARCHAR(30) NULL DEFAULT NULL , `state` VARCHAR(30) NULL DEFAULT NULL , `password` VARCHAR(255) NULL DEFAULT NULL , `created_at` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP , `updated_at` TIMESTAMP NULL DEFAULT NULL , PRIMARY KEY (`id`)) ENGINE = MyISAM;